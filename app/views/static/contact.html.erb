<h1 class = "page-header"> contact us </h1>


<%= form_for(@contact, url: contact_path) do |f| %>
  <%= f.text_field(:name, placeholder: "Name") %> <br>
  <%= f.email_field(:email, placeholder: "Email") %><br>
  <%= f.text_area(:question, placeholder: "Question") %><br>
  <%= f.submit("Say Aye") %>
<% end %>